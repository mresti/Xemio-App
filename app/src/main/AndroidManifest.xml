    <activity
        android:name="es.mresti.xemio.app.view.activity.CancerActivity"
        android:label="@string/app_name"
        android:theme="@style/Theme.Xemio.Init"
        >
    </activity>

    <activity
        android:name="es.mresti.xemio.app.view.activity.ChemoActivity"
        android:label="@string/app_name"
        android:theme="@style/Theme.Xemio.Init"
        >
    </activity>

    <activity
        android:name="es.mresti.xemio.app.view.activity.PassActivity"
        android:label="@string/app_name"
        android:theme="@style/Theme.Xemio.Init"
        >
    </activity>

    <activity
        android:name="es.mresti.xemio.app.view.activity.IncidenceActivity"
        android:label="@string/toolbar_new_incidence"
        >
    </activity>

    <activity
        android:name="es.mresti.xemio.app.view.activity.SettingsActivity"
        android:label="@string/action_settings"
        >
    </activity>

    <activity
        android:name="es.mresti.xemio.app.view.activity.InfoActivity"
        android:label="@string/info_title"
        >
    </activity>

  </application>

</manifest>
